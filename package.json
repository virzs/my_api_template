    "@nestjs/common": "^10.4.9",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^10.4.9",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.7",
    "axios": "^1.7.8",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
