    "dompurify": "^3.2.1",
    "express-rate-limit": "^7.4.1",
    "jsdom": "^25.0.1",
    "log4js": "^6.9.1",
    "marked": "^15.0.2",
    "mongoose": "^8.8.3",
    "ms": "^2.1.3",
    "nodemailer": "^6.9.16",
    "qiniu": "^7.14.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stacktrace-js": "^2.0.2",
    "uuid": "^11.0.3"
